{
   "extends":[
      "config:base",
      ":automergePatch"
   ],
   "packageRules":[
      {
         "updateTypes":[
            "major",
            "minor",
            "patch"
         ],
         "addLabels":[
            "major",
            "minor",
            "patch"
         ]
      },
      {
         "packageNames":[
            "RSTUDIO_VERSION"
         ],
         "updateTypes":[
            "pin",
            "lockFile",
            "range"
         ],
         "repository":"rstudio/rstudio",
         "extends":[
            {
               "file":"rstudio/rstudio_vars.yml",
               "versionKey":"RSTUDIO_VERSION"
            }
         ]
      },
      {
         "packageNames":[
            "VSCODE_VERSION"
         ],
         "updateTypes":[
            "pin",
            "lockFile",
            "range"
         ],
         "repository":"cdr/code-server",
         "extends":[
            {
               "file":"vscode/vscode_vars.yml",
               "versionKey":"VSCODE_VERSION"
            }
         ]
      }
   ],
   "vulnerabilityAlerts":{
      "labels":[
         "security"
      ]
   },
   "baseBranches":[
      "dev"
   ],
   "reviewers":[
      "team:portal-dev"
   ],
   "labels":[
      "dependencies"
   ],
   "assignees":[
      "team:portal-dev"
   ],
   "branchPrefix":"deps/",
   "bumpVersion":"minor",
   "rebaseWhen":"behind-base-branch",
   "commitBodyTable":"true",
   "commitMessagePrefix":"feat(Dependencies):"
}
