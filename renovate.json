{
   "extends":[
      "config:base",
      ":automergePatch"
   ],
   "platform":"github",
   "regexManagers":[
      {
         "fileMatch":[
            "rstudio/rstudio_vars_file\\.yml"
         ],
         "matchStrings":[
            "RSTUDIO_VERSION=(?<currentDigest>.*?)\\n"
         ],
         "depNameTemplate":"https://github.com/rstudio/rstudio",
         "datasourceTemplate":"github-tags"
      }
   ],
   "packageRules":[
      {
         "matchPackageNames":[
            "RSTUDIO_VERSION"
         ],
         "matchUpdateTypes":[
            "major",
            "minor",
            "patch"
         ],
         "addLabels":[
            "major",
            "minor",
            "patch"
         ],
         "repository":"rstudio/rstudio",
         "extends":[
            {
               "file":"rstudio/rstudio_vars_file.yml",
               "versionKey":"RSTUDIO_VERSION",
               "property":"rstudio_vars"
            }
         ]
      },
      {
         "matchPackageNames":[
            "VSCODE_VERSION"
         ],
         "matchUpdateTypes":[
            "major",
            "minor",
            "patch"
         ],
         "addLabels":[
            "major",
            "minor",
            "patch"
         ],
         "repository":"coder/code-server",
         "extends":[
            {
               "file":"vscode/vscode_vars_file.yml",
               "versionKey":"VSCODE_VERSION",
               "property":"vscode_vars"
            }
         ]
      }
   ],
   "vulnerabilityAlerts":{
      "labels":[
         "security"
      ]
   },
   "baseBranches":[
      "dev"
   ],
   "reviewers":[
      "team:portal-dev"
   ],
   "labels":[
      "dependencies"
   ],
   "assignees":[
      "team:portal-dev"
   ],
   "branchPrefix":"deps/",
   "bumpVersion":"minor",
   "rebaseWhen":"behind-base-branch",
   "commitBodyTable":true,
   "commitMessagePrefix":"feat(Dependencies):"
}
